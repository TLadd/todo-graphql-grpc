{
  "name": "todo-graphql-grpc",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.1.15",
    "graphql": "^0.13.2",
    "react": "^16.4.2",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.2",
    "react-scripts": "2.0.0-next.3e165448",
    "todomvc-app-css": "^2.1.2",
    "todomvc-common": "^1.0.5"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "genQueries": "apollo schema:download --endpoint=http://localhost:4000/graphql schema.json && apollo codegen:generate --schema=schema.json --target=flow --outputFlat src/types.js && apollo-flow-components",
    "genTypes": "apollo schema:download --endpoint=http://localhost:4000/graphql schema.json && apollo codegen:generate --schema=schema.json --target=typescript --outputFlat src/types.ts"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.0.0",
    "apollo": "^1.9.1",
    "apollo-flow-components": "^1.0.0",
    "react-app-rewire-typescript-babel-preset": "^2.4.0",
    "react-app-rewired": "next",
    "typescript": "^3.0.3"
  }
}
